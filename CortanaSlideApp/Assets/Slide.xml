<?xml version="1.0" encoding="utf-8"?>

<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.1">
  <CommandSet xml:lang="en-US">
    <CommandPrefix>CortanaSlide</CommandPrefix>
    <Example>Can I finish this talk ?</Example>


    <Command Name="voiceCommand1">
      <Example>Can I finish this presentation ?</Example>
      <ListenFor> [Can I finish] {msgText} </ListenFor>
      <Feedback> Sure, I try to finish {msgText} </Feedback>
      <Navigate Target="SendPage.xaml"/>
    </Command>

    <PhraseTopic Label="msgText" Scenario="Short Message"/>
	
  </CommandSet>
</VoiceCommands>